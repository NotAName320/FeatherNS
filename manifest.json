{
  "manifest_version": 3,
  "name": "Feather",
  "version": "2.3",
  "description": "Feather, fork of SweezeBreeze tailored for usage with TBH.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
